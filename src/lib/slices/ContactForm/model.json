{
  "id": "contact_form",
  "type": "SharedSlice",
  "name": "ContactForm",
  "description": "Simple contact form for counseling inquiries",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "Text",
          "config": {
            "label": "Title",
            "placeholder": "Get In Touch"
          }
        },
        "subtitle": {
          "type": "Text",
          "config": {
            "label": "Subtitle",
            "placeholder": "Ready to start your healing journey?"
          }
        },
        "form_header_title": {
          "type": "Text",
          "config": {
            "label": "Form Header Title",
            "placeholder": "Schedule Your First Session"
          }
        },
        "form_header_description": {
          "type": "Text",
          "config": {
            "label": "Form Header Description",
            "placeholder": "Please share some information about yourself and what brings you here today."
          }
        },
        "reception_email": {
          "type": "Text",
          "config": {
            "label": "Reception Email",
            "placeholder": "example@service.com"
          }
        },
        "services": {
          "type": "Group",
          "config": {
            "label": "Services",
            "repeat": true,
            "fields": {
              "name": {
                "type": "Text",
                "config": {
                  "label": "Name",
                  "placeholder": ""
                }
              }
            }
          }
        }
      },
      "items": {}
    }
  ]
}