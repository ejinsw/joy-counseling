{
  "id": "services",
  "type": "SharedSlice",
  "name": "Services",
  "description": "Services offered by the counseling practice",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "Text",
          "config": {
            "label": "Title",
            "placeholder": "Our Services"
          }
        },
        "subtitle": {
          "type": "Text",
          "config": {
            "label": "Subtitle",
            "placeholder": "Professional counseling services to help you heal and grow"
          }
        }
      },
      "items": {
        "title": {
          "type": "Text",
          "config": {
            "label": "Service Title",
            "placeholder": "Individual Therapy"
          }
        },
        "description": {
          "type": "StructuredText",
          "config": {
            "label": "Service Description",
            "placeholder": "Describe what this service offers and how it helps clients",
            "allowTargetBlank": true,
            "multi": "paragraph,preformatted,heading1,heading2,heading3,strong,em,hyperlink,embed,list-item"
          }
        },
        "icon": {
          "type": "Select",
          "config": {
            "label": "Service Icon",
            "options": [
              "heart",
              "brain",
              "conversation",
              "growth",
              "family",
              "couple",
              "mindfulness",
              "healing",
              "support",
              "wellness"
            ]
          }
        },
        "cta_text": {
          "type": "Text",
          "config": {
            "label": "Call to Action Text",
            "placeholder": "Book Session"
          }
        },
        "cta_link": {
          "type": "Link",
          "config": {
            "label": "Call to Action Link",
            "placeholder": "",
            "select": null,
            "allowTargetBlank": false,
            "allowText": false,
            "repeat": false
          }
        }
      }
    }
  ]
}